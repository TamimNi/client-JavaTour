<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="at.fhtw.swen2.tutorial.presentation.view.NewTourWindow"
            prefHeight="400.0" prefWidth="600.0">


    <children>
        <Label fx:id="NewTourWindowTitleLabel" layoutX="14.0" layoutY="14.0" prefHeight="23.0" prefWidth="292.0" text="New Tour" textAlignment="CENTER">
        </Label>
        <Label layoutX="14.0" layoutY="63.0" prefHeight="17.0" prefWidth="78.0" text="Name:" />
        <TextField fx:id="nameTextField" layoutX="98.0" layoutY="58.0" prefHeight="25.0" prefWidth="203.0" />
        <Label layoutX="14.0" layoutY="105.0" prefHeight="17.0" prefWidth="78.0" text="Tour description:" />
        <TextField fx:id="tourDescriptionTextField" layoutX="98.0" layoutY="100.0" prefHeight="25.0" prefWidth="203.0" />
        <Label layoutX="14.0" layoutY="147.0" prefHeight="17.0" prefWidth="78.0" text="From:" />
        <TextField fx:id="fromTextField" layoutX="98.0" layoutY="142.0" prefHeight="25.0" prefWidth="203.0" />
        <Label layoutX="14.0" layoutY="189.0" prefHeight="17.0" prefWidth="78.0" text="To:" />
        <TextField fx:id="toTextField" layoutX="98.0" layoutY="184.0" prefHeight="25.0" prefWidth="203.0" />
        <Label layoutX="14.0" layoutY="231.0" prefHeight="17.0" prefWidth="78.0" text="Transport Type:" />
        <TextField fx:id="transportTypeTextField" layoutX="98.0" layoutY="226.0" prefHeight="25.0" prefWidth="203.0" />
        <Button fx:id="submitTourButton" layoutX="119.0" layoutY="277.0" onAction="#submitTourButtonAction" mnemonicParsing="false" text="Submit" />
    </children>


</AnchorPane>
